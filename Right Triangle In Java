import java.util.*;
class Right   
{   
public static void main(String [] args)   
{     
int i, j;
Scanner k = new Scanner (System.in);
System.out.print("Enter a number : ");
int n = k.nextInt();
for(i=0; i<n; i++)   
{ 
for(j=0; j<=i; j++)   
{      
System.out.print("* ");   
}   
System.out.println();   
}   
}   
}
